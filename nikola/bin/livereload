# EASY-INSTALL-ENTRY-SCRIPT: 'livereload==1.0.1','console_scripts','livereload'
__requires__ = 'livereload==1.0.1'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('livereload==1.0.1', 'console_scripts', 'livereload')()
    )
